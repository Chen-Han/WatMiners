{
  "rules": {
    "rankings": {
      "$uid": {
        ".read": "auth != null && auth.uid == $uid",
        ".write": "auth != null && auth.uid == $uid",
        ".validate": "newData.hasChildren(['jobID','ranking','preference','displayName'])",
        "ranking": {
          ".validate": "newData.isString() &&(newData.val()=='Ranked'|| newData.val()=='Offered')"
        },
        "preference": {
          ".validate": "newData.isNumber() && newData.val()>=-1 && newData.val()<=9"
        },
        "jobID": {
          ".validate": "newData.isNumber()"
        },
        "displayName": {
          ".validate": "newData.isString()"
        }
      },
      "ranking":{

      },
      "preference":{

      },
      "jobID":{},
      "displayName":{

      }
    }
  }
}